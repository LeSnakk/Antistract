<UserControl x:Class="antistract.MVVM.View.PlansView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:antistract.MVVM.View" xmlns:local1="clr-namespace:antistract.Core"
             mc:Ignorable="d" 
             d:DesignHeight="570" d:DesignWidth="710">

    <Border Padding="35,0,0,0" 
            Margin="0,564.75,0,0" 
            Background="#ffffff"
            CornerRadius="0,0,7,0">

        <Border CornerRadius="31,7,7,0"
                Padding="0,30,0,0"
                Margin="0,-565,0,0">

            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <LinearGradientBrush.RelativeTransform>
                        <TransformGroup>
                            <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                            <SkewTransform CenterX="0.5" CenterY="0.5"/>
                            <RotateTransform Angle="-87" CenterX="0.5" CenterY="0.5"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </LinearGradientBrush.RelativeTransform>
                    <GradientStop Color="#ffffff"/>
                    <GradientStop Color="#ffffff" Offset="0.304"/>
                </LinearGradientBrush>
            </Border.Background>

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2.5*" />
                    <ColumnDefinition Width="7.5*" />
                </Grid.ColumnDefinitions>
                <Border x:Name="MenuArea">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*" />
                            <RowDefinition Height="8*" />
                            <RowDefinition Height="1*" />
                        </Grid.RowDefinitions>

                        <StackPanel>
                            <TextBlock Grid.Row="0" 
                               VerticalAlignment="Center"
                               HorizontalAlignment="Center"
                               FontFamily="/Fonts/#Dosis"
                               FontWeight="SemiBold"
                               FontSize="30">
                        My Plans
                            </TextBlock>
                            <Button x:Name="AddPlanButton"
                                    Style="{StaticResource StandardSmallButtonTheme}"
                                    HorizontalAlignment="Center"
                                    Click="AddPlanButton_Click">
                                Add Plan
                            </Button>

                            <Button x:Name="GoToPlansViewButton" Command="{Binding DataContext.PlansViewCommand, RelativeSource={RelativeSource AncestorType={x:Type Window}}}"
                        Visibility="Hidden"></Button>
                        </StackPanel>

                        <ScrollViewer Grid.Row="1" Margin="2,10,10,10"
                                      VerticalScrollBarVisibility="Auto">

                            <ItemsControl ItemsSource="{Binding}" x:Name="PlanOverviewStackPanel" Grid.Column="0">
                                <ItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel Orientation="Vertical"/>
                                    </ItemsPanelTemplate>
                                </ItemsControl.ItemsPanel>
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <RadioButton Content="{Binding entryName}"
                                                     GroupName="PlanSelection"
                                                     Style="{StaticResource PlansListButtonTheme}"
                                                     IsChecked="{Binding isChecked, Mode=TwoWay}"
                                                     Click="PlanEntryNameList_Click"/>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                        </ScrollViewer>

                        <Grid Grid.Row="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*" />
                                <ColumnDefinition Width="3*" />
                                <ColumnDefinition Width="3*" />
                            </Grid.ColumnDefinitions>

                            <Button Grid.Column="0"
                                    Style="{StaticResource StandardSmallButtonTheme}"
                                    x:Name="DeleteButton"
                                    FontFamily="/Fonts/#Dosis"
                                        FontSize="16"
                                    Margin="2"
                                    Click="Delete_Button_Click"
                                    IsEnabled="False">
                                Delete
                            </Button>
                            <Button Grid.Column="1"
                                    Style="{StaticResource StandardSmallButtonTheme}"
